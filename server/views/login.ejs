<html>	<head>		<title>			Image Gallery		</title>		<link href="jquery/css/smoothness/jquery-ui-1.9.2.custom.min.css" rel="stylesheet" type="text/css"/>		<link href="style.css" rel="stylesheet" type="text/css"/>		<script src="jquery/js/jquery-1.8.3.js"></script>		<script src="jquery/js/jquery-ui-1.9.2.custom.min.js"></script>		<script src="js/login.js"></script>		<script src="js/messages.js"></script>			<script type="text/javascript">			$(function() {				$( "input[type=submit], a, button" ).button();			});		</script>	</head>	<body background="background.jpg">		<% include messages %>				<div align="center">			Login <br />			<form action="/login" id="loginForm" method="post"><br />				Username: <input id="usernameBox" type="text"><br />				Password: <input id="passwordBox" type="password"><br />				<a id="loginButton">login</a>			</form>		</div>		<div align="center">			Register			<form action="/register" id="registerForm" method="post">				Username: <input type="text" id="registerUsernameBox"><br />				Password: <input type="password" id="registerPasswordBox"><br />				<a id="registerButton">register</a>			</form>		</div>	</body></html>